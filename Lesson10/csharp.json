{
	// Place your snippets for csharp here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	"Custom Class": {
		"prefix": ["cclass", "custom-class"],
		"body": ["namespace Namespace", "{", "\tclass Name", "\t{\n", "\t\t// Fields\n", "\t\t// Properties\n", "\t\t// Constructor\n", "\t\t// Methods\n", "\t}", "}"],
		"description": "A Custom Class."
	  },

	"Custom Abstract Class": {
		"prefix": ["aclass", "custom-abstract-class"],
		"body": ["namespace Namespace", "{", "\tclass Name", "\t{\n", "\t\t// Fields\n", "\t\t// Properties\n", "\t\t// Constructor\n", "\t\t// Methods\n", "\t}", "}"],
		"description": "A Custom Abstract Class."
	  },

	"Custom Static Class": {
		"prefix": ["sclass", "custom-static-class"],
		"body": ["namespace Namespace", "{", "\tstatic class Name", "\t{\n", "\t\t// Methods\n", "\t}", "}"],
		"description": "A Custom Static Class."
	  },

	"Custom Interface": {
		"prefix": ["iinterface", "custom-interface"],
		"body": ["namespace Namespace", "{", "\tinterface IName", "\t{\n", "\t\t// Methods\n", "\t}", "}"],
		"description": "A Custom Interface."
	}
}